<script>
  window.onload = function(){
	document.getElementById("freqDist").src = getUrl("freqdist.html");
	document.getElementById("cluscoeff").src = getUrl("cluscoeff.html");
 }
  function getUrl(rootUrl) {
	var cuisine = getQueryVariable("cuisine");
	var url = rootUrl + "?cuisine=" + cuisine ;
	return url;
  }	
  </script>
</head>
<body>
	<div>
		<div style="width: 400px; float: left;">
  			<h2>Ingredient Frequency </h2>
			<iframe id='freqDist' scrolling="no" allowfullscreen width=400 height=400 frameborder="0"></iframe>
		</div>
		<div style="margin-left: 400px;">
  			<h2>Clustering Coefficient</h2>
			<iframe id='cluscoeff' scrolling="no" allowfullscreen width=400 height=400 frameborder="0"></iframe>
		</div>
	</div>
</div>
</body>
</html>

